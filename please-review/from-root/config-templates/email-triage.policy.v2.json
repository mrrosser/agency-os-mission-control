  "triage": {
    "searchQuery": "is:unread in:inbox newer_than:7d -category:promotions -category:social -category:updates",
    "suppression": {
      "skipIfFromContains": [
        "no-reply",
        "noreply",
        "donotreply",
        "mailer-daemon",
        "postmaster"
      ],
      "skipIfSubjectContains": [
        "receipt",
        "invoice paid",
        "payment confirmation",
        "verification code",
        "password reset",
        "newsletter",
        "digest",
        "meeting notes",
        "your note",
        "calendar summary"
      ],
      "skipIfHeadersPresent": [
        "Auto-Submitted",
        "List-Id",
        "Precedence: bulk"
      ]
    },
    "intents": {
      "draftMinConfidence": 0.72,
      "needsReplyMinConfidence": 0.68,
      "quoteMinConfidence": 0.7,
      "meetingRequestMinConfidence": 0.84
    },
    "actions": {
      "createDraftWhenConfidenceAtLeast": 0.72,
      "createDraftForUnknownIntent": false,
      "applyNeedsReplyLabel": true,
      "applyQuoteLabel": true,
      "postDigestForSkipped": true
    },
    "testing": {
      "allowCrossMailboxInternalSenders": true,
      "selfMailPolicy": "skip_unless_subject_starts_with_marker",
      "selfMailMarkerPrefix": "[triage-test]"
    }
  },
